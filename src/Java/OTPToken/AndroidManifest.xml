<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.java.androidtoken"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="9" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon2"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.java.androidtoken.SplashActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.java.androidtoken.AddTokenActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.java.androidtoken.SetPINActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.java.androidtoken.PINLoginActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.java.androidtoken.TokenListActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>